<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vpteruel.cubecalculator.CubeCalculatorController">
    <TextField fx:id="numberInput" promptText="Enter a number" />

    <Button text="Calculate Cube" onAction="#calculateCube" />

    <Label fx:id="resultLabel" />
</VBox>
